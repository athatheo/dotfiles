{
    "files.autoSave": "onFocusChange",
    "files.autoSaveDelay": 10000,
    "editor.fontSize": 13,
    "editor.wordWrapColumn": 92,
    "editor.letterSpacing": 0.2,
    "editor.smoothScrolling": true,
    "editor.minimap.enabled": false,
    "editor.suggest.localityBonus": true,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.suggest.maxVisibleSuggestions": 10,
    "editor.suggest.shareSuggestSelections": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "workbench.panel.defaultLocation": "right",
    "zenMode.hideLineNumbers": false,
    "zenMode.hideTabs": false,
    "terminal.external.linuxExec": "gnome-terminal",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.scrollback": 10000,
    "telemetry.enableTelemetry": false,
    "go.toolsGopath": "~/go/",
    "markdown.preview.fontSize": 13,
    "vim.camelCaseMotion.enable": true,
    "vim.useSystemClipboard": true,
    "vim.vimrc.enable": true,
    "vim.handleKeys": {
        "<C-w>": false,
        "<C-j>": false,
        "<C-`>": false,
        "<C-k>": false,
    },
    "debug.console.fontSize": 13,
    "window.menuBarVisibility": "toggle",
    "todo-tree.highlights.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "todo-tree.tree.showScanModeButton": false,
    "editor.insertSpaces": false,
    "liveshare.allowGuestDebugControl": true,
    "liveshare.guestApprovalRequired": true,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.defaultDateFormat": "YYYY/MM/DD hh:mm",
    "gitlens.defaultDateShortFormat": "YY/MM/DD:hh",
    "diffEditor.renderSideBySide": false,
    "julia.enableTelemetry": true,
    "julia.format.indent": 2,
    "julia.useCustomSysimage": true,
    "julia.enableCrashReporter": true,
    "explorer.confirmDelete": false,
    "vim.commandLineModeKeyBindings": [

    ],
    "git.autofetch": true,
    "editor.codeActionsOnSave": null,
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.execution.resultType": "both",
    "julia.NumThreads": 10,
    "workbench.colorCustomizations": {
        "statusBar.background": "#005f5f",
        "statusBar.noFolderBackground": "#005f5f",
        "statusBar.debuggingBackground": "#005f5f"
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "go.toolsManagement.autoUpdate": true,
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast"
    ],
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "redhat.telemetry.enabled": true,
    "julia.symbolCacheDownload": true,
    "tabnine.experimentalAutoImports": true,
    "window.zoomLevel": -1
}
